{
  "elasticsearch": {
    "ripsaw-kube-burner": [
      {
        "filter": {
          "metricName.keyword": "containerCPU",
          "labels.namespace.keyword": "${TOUCHSTONE_NAMESPACE}"
        },
        "buckets": [
          "labels.pod.keyword",
          "labels.node.keyword"
        ],
        "aggregations": {
          "value": [
            "avg"
          ]
        }
      }
    ]
  }
}
