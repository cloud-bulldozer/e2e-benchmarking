{
    "elasticsearch": {
        "metadata": {
            "prod-netobserv-operator-metadata": {
                "fields": [
                    "agent",
                    "cache_active_time",
                    "cache_max_flows",
                    "flp_kind",
                    "release",
                    "sampling"
                ],
                "additional_fields": [
                    "metric_name"
                ]
            },
            "prod-netobserv-jenkins-metadata": {
                "fields": [
                    "jenkins_build_num",
                    "jenkins_job_name",
                    "workload",
                    "variable"
                ],
                "additional_fields": [
                    "metric_name"
                ]
            }
        },
        "prod-netobserv-datapoints": [
            {
                "filter": {
                    "metric_name.keyword": "nFlowsProcessedPerMinute"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        {
                            "percentiles": {
                                "percents": [
                                    90
                                ]
                            }
                        },
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "nBytesProcessedPerMinute"
                },
                "buckets": [
                    "data_type.keyword"
                ],
                "aggregations": {
                    "value": [
                        {
                            "percentiles": {
                                "percents": [
                                    90
                                ]
                            }
                        },
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "nPacketsProcessedPerMinute"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        {
                            "percentiles": {
                                "percents": [
                                    90
                                ]
                            }
                        },
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "lokiStorageUsage"
                },
                "buckets": [
                    "metadata.persistentvolumeclaim.keyword"
                ],
                "aggregations": {
                    "value": [
                        "max"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "cpuUsage"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "memoryUsageRSS"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "memoryUsageWorkingSet"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "ebpfMemoryUsageRSS"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        "avg"
                    ]
                }
            },
            {
                "filter": {
                    "metric_name.keyword": "ebpfMemoryUsageWorkingSet"
                },
                "buckets": [
                    "metadata.pod.keyword"
                ],
                "aggregations": {
                    "value": [
                        "avg"
                    ]
                }
            }
        ]
    }
}